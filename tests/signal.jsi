#!/usr/local/bin/jsish -u %s

if (!Signal) throw('signal unsupported');
;Signal.names().sort();
function mysig() {
  puts("MYSIG");
  exit(0);
}

var i = Util.getpid();
//Signal.handle('USR1');
Signal.callback(mysig,'USR1');

Signal.kill(Util.getpid(),'USR1');
while (true) {
   Event.update(1000);
}

