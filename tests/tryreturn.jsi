#!/usr/local/bin/jsish -u %s

function f(n) {
  try {
     if (n==1) return n;
     puts('TRY');
     if (n!=0)
        throw(9);
  } catch(e) {
     puts("CATCH");
     if (n==2) return n;
     puts("ERR: "+e);
  } finally {
    puts("FINALLY");
    if (n==3)
      return n;

  }
  puts("OK");
  return 1;
}
;f(0);
;f(1);
;f(2);
;f(3);

